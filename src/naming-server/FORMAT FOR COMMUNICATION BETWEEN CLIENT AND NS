FORMAT FOR COMMUNICATION BETWEEN CLIENT AND NS

Format (to NS)	Format (from NS)
CREATE\n<folderpath>\n<f_path (file or folder)>\n	<success or failure message>\n
DELETE\n<folderpath>\n<f_path (file or folder)>\n	<success or failure message>\n
COPY\n<sourcefilepath>\n<destfolderpath>\n	<success or failure message>\n
INFO\n<filepath>\n	<info about file>\n<success or failure message>\n
LIST\n<folderpath>\n	<full list of items>\n<success or failure message>\n
READ\n<filepath>\n	<ss_ip>\n<ss_portnum>\n
STREAM\n<audiofilepath>\n	<ss_ip>\n<ss_portnum>\n
WRITE\n<destfilepath>\n	<ss_ip>\n<ss_portnum>\n
